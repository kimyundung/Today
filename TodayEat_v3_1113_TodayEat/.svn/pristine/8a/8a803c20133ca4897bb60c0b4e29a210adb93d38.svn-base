@charset "UTF-8";
@import url('https://fonts.googleapis.com/css?family=Nanum+Gothic');


* { 
   margin: 0;
   padding: 0;
   
}


body {
   font: 'Nanum Gothic', sans-serif;
   
}
#page{
   margin-top: 210px;
   height: 100%;
   min-width: 800px;
}


#jb-container:after{content:''; display:block; clear:both; float:none;}
      

#jb-container{
   
   width: 940px;
   margin-right: auto;
    margin-left: auto;
    margin-bottom: 90px;
   
}

#jb-header{
   
   margin-bottom: 20px;
   
}

#jb-content{
   width: 600px;
    margin-top: 100px;
    margin-bottom: 20px;
    margin-right: auto;
    margin-left: auto;
    float: none;
    padding: 50px 20px 20px 020px;
    height: 1000px;
}


#menu_nav a{
   text-decoration: none;
   color: black;
/*    padding-top: 30px;
 */   
}

li {
   list-style: none;
}

#menu_nav{
   
   font: 5px;
   width: 500px;
   height: 50px;

}

#menu_nav ul{
   width: 500px;
   overflow: hidden;
   margin:0 auto;
}



#menu_nav ul li {
   float: left;
   width: 25%;
   height: 100%;
   line-height: 30px;
   text-align: center;
}

#menu_nav ul li a{
   display: block;
   border-radius: 10px;
}

#menu_nav ul li a:hover{
   background: #0B6623;   
   color: #fff;
   
}


.current{
   color: #0B6623;
   border-bottom: 3px solid #0B6623;
}


.allCheck_btn {
   width: 100px;
    height: 30px;
    background-color: #0B6623;
    border-radius: 5px;
    border: none;
    color:#fff;
    padding: 3px 0;
    text-align: center;
    text-decoration: none;
    display: inline-block;
    font-size: 12px;
    margin: 3px;
    cursor: pointer;
    position: relative;
    left: 600px;
    top: -30px;
}

.selectDelete_btn{
   width: 100px;
    height: 30px;
    background-color: #0B6623;
    border-radius: 5px;
    border: none;
    color:#fff;
    padding: 3px 0;
    text-align: center;
    text-decoration: none;
    display: inline-block;
    font-size: 12px;
    margin: 3px;
    cursor: pointer;
    position: relative;
    left: 600px;
    top: -30px;
}

h3{
   font-weight: bolder;
    color: #0B6623;
    font-size: 20px;
}

p.x{
   margin-top:5px;
   font-weight: bold;
    font-size: 15px;
}
p.e{
   margin-top:5px;
}

.ex_thumb{
   margin-top:5px;
}

.recipe1, .recipe2, .recipe3, .recipe4, .recipe5{
   float:left;
   margin-right: 15px;
}  

td{
   padding-top: 30px;
   padding-bottom:15px;
   border-bottom-style:inset;
}


strong.green {
    color: #0B6623;
}

strong.red{
   color:red;
}

b.green{

    color: #0B6623;
}

b.red{
   color:red;
}

span.em_recipe {
    color: #bdbdbd;
    font-size: 20px;
    font-style: italic;
    margin-left: 230px;
}

.footer {
    position: static;
    bottom: 0;
    width: 100%;
    height: 90px;
    background-color: #0B6623;
    min-width: 800px;
    margin-top: 450px;
}